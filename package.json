{
  "name": "spkpeminatan",
  "version": "1.0.0",
  "description": "Sistem Pendukung Keputusan Peminatan Kuliah Berdasarkan Nilai Matakuliah dengan Metode GED &amp; Algoritma Greedy",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NazirArifin/spkpeminatan.git"
  },
  "keywords": ["spk", "sistem pendukung keputusan", "dss", "decision support system", "peminatan", "penjurusan"],
  "author": "Mohammad Nazir Arifin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NazirArifin/spkpeminatan/issues"
  },
  "homepage": "https://github.com/NazirArifin/spkpeminatan#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "jest": "^27.2.0",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3"
  }
}
